ORG 0000H

MOV A, #00H ; CHANGE TO 55H FOR ALTERNATE ON OFF.

LOOP: CPL A
    MOV P0, A
    ACALL DELAY
    SJMP LOOP


DELAY: MOV R7, #50H
    H1: MOV R2,#255H
    H2: MOV R3,#255H
    L:DJNZ R3,L
    DJNZ R2,H2
    DJNZ R7,H1
    RET
END


DELAY: MOV R7, #50H
    D2: MOV R8, #255H
    D3: MOV R9, #255H
    PUSH 
    POP 
    NOP
    NOP
    DJNZ R9, D3
    DJNZ R8, D2
    DJNZ R7, DELAY
    RET